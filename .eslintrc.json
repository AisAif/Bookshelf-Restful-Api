{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "off",
        "quotes": "off",
        "no-console": "off",
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
